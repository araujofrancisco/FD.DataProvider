<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Francisco Araujo</Authors>
    <PackageId>FreelanceDevelopers.SampleData</PackageId>
    <Description>Library for adding sample data context to test applications, use sqlite in memory database to keep data and allow initial seeding.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/araujofrancisco/FD.DataProvider/tree/master/FD.SampleData</PackageProjectUrl>
    <RepositoryUrl>https://github.com/araujofrancisco/FD.DataProvider</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>sqlite, dbcontextfactory, data-generators</PackageTags>
    <PackageReleaseNotes>- Add DbConnectionRestricted class to share a subset of methods for the database connection, allowing access to create dbcommands and run raw queries.
</PackageReleaseNotes>
    <Version>1.0.5.4</Version>
    <AssemblyVersion>1.0.5.4</AssemblyVersion>
    <FileVersion>1.0.5.4</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FreelanceDevelopers.Blazor.Core" Version="1.0.8.3" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="5.0.8" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
